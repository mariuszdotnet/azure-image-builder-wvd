{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "keyVaultName": {
            "type": "string"
        },
        "secretsObject": {
            "type": "secureObject",
           "defaultValue": [
          {
            "secretName": "subnet-id",
            "secretValue": "secretVaule1"
          },
          {
            "secretName": "subnetid-netapp",
            "secretValue": "secretValue2"
          },
          {
              "secretName":"admin-account-password",
              "secretVale":""
          }
        ],
        "metadata": {
        "description": "Specifies all secrets {\"secretName\":\"\",\"secretValue\":\"\"} wrapped in a secure object."
      }
        }
    },
    "functions": [],
    "variables": {},
    "resources": [
            {
      "type": "Microsoft.KeyVault/vaults/secrets",
      "name": "[concat(parameters('keyVaultName'), '/', parameters('secretsObject').secrets[copyIndex()].secretName)]",
      "apiVersion": "2018-02-14",
      "dependsOn": [
        "[concat('Microsoft.KeyVault/vaults/', parameters('keyVaultName'))]"
      ],
      "copy": {
        "name": "secretsCopy",
        "count": "[length(parameters('secretsObject').secrets)]"
      },
      "properties": {
        "value": "[parameters('secretsObject').secrets[copyIndex()].secretValue]"
      }
    }
    ],
    "outputs": {}
}