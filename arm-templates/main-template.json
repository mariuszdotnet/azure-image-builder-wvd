{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "utc-value": {
            "type": "string",
            "defaultValue": "[utcNow()]"
        }
    },
    "functions": [],
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-10-01",
            "name": "[concat('compute-availabilitysets-',parameters('utc-value'))]",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri":"https://raw.githubusercontent.com/mariuszdotnet/azure-image-builder-wvd/master/arm-templates/compute-availabilitysets.json",
                    "contentVersion":"1.0.0.0"
                }
            }
        },
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-10-01",
            "name": "[concat('network-publicipaddresses-',parameters('utc-value'))]",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri":"https://raw.githubusercontent.com/mariuszdotnet/azure-image-builder-wvd/master/arm-templates/network-publicipaddresses.json",
                    "contentVersion":"1.0.0.0"
                }
            }
        }
    ],
    "outputs": {}
}